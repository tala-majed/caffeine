useState({ currentUser: null, isLoggedIn: false });\n  const [selectProduct, setSelectProduct] = useState({});\n  const [selectArtcile, setSelectArticle] = useState({});\n  const [articlePage, setArticlePage] = useState(false)\n  const [productPage, setProductPage] = useState(false)\n  const[productLenght, SetProductLenght] = useState(0)\n  const [search, setSearch] = useState(\"\");\n\n  const userLogin = () => {\n    if (localStorage.jwtToken) {\n      const jwtToken = localStorage.jwtToken;\n      const currentUser = jwt_decode(jwtToken, process.env.SECRET_KEY).user;\n      setAuth({ currentUser, isLoggedIn: true });\n    } else {\n      setAuth({ currentUser: null, isLoggedIn: false });\n    }\n\n    setDataloading(true);\n    console.log(\"The current User is: \", auth.currentUser);\n  };\n\n  useEffect(userLogin, []);\n\n\n  const ToSetSearch = (text)=>{ setSearch(text)}\n\n  \n  return (\n    <div className=\"\">\n\n      {dataLoading && (\n        <Router>\n          <NavBar\n            isLoggedIn={auth.isLoggedIn}\n            loginCallback={userLogin}\n            setArticlePage={setArticlePage}\n            articlePage={articlePage}\n            setProductPage={setProductPage}\n            productPage={productPage}\n            ToSetSearch={ToSetSearch}\n            productLenght={productLenght}\n            />\n          <Switch \n          >\n          <Route exact  path=\"/\">\n\n            <HomePage\n             setSelectArticle={setSelectArticle}\n             />\n          </Route>\n\n          <Route exact path=\"/login\">\n            <Login \n            loginCallback={userLogin} />\n          </Route>\n\n          <Route exact path=\"/signup\">\n            <Signup \n            loginCallback={userLogin} />\n          </Route>\n\n          <Route exact path=\"/forgot\">\n\n\n\n            <Forgot \n            loginCallback={userLogin} />\n\n\n          </Route>\n\n\n          <Route exact path=\"/new-product\">\n            <NewProduct \n            setAuth={setAuth}\n              auth={auth} />\n\n          </Route>\n\n          <Route path=\"/products/:id\">\n            <ShowOneProduct \n            selectProduct={selectProduct}\n             auth={auth} />\n\n          </Route>\n\n          <Route exact path=\"/products\">\n           \n            <Products \n            setSelectProduct={setSelectProduct}\n             search={search}/>\n          </Route>\n\n          <Route exact path=\"/cart\">\n            <Cart  \n            auth={auth}\n            SetProductLenght={SetProductLenght} />\n          </Route>\n\n          \n          <Route path=\"/new-article\">\n            <NewArticle \n            setAuth={setAuth}\n              auth={auth}/>\n          </Route>\n\n          <Route path=\"/:article_id/article\" >\n            <ShowOneArticle \n            selectArtcile={selectArtcile} auth={auth}/>\n          </Route>\n\n          <Route path=\"/articles\" >\n            <Articles\n             />\n          </Route>\n\n          <Route path=\"/admin\" >\n            <Admin \n            auth={auth}/>\n          </Route>\n\n\n\n          <Route exact path=\"/profile\" >\n            < UserProfile \n            setAuth = {setAuth}\n            auth={auth}/>\n          </Route>\n\n\n          </Switch>\n          <Footer />\n\n\n        </Router>\n        \n      )}\n\n    </div>\n  );\n}\n\nexport default  App;\n","/Users/tala/Desktop/new/caffeine/caffeine-react/src/apiConfig.js",[],"/Users/tala/Desktop/new/caffeine/caffeine-react/src/pages/Home.jsx",["107","108","109","110","111"],"import API_URL from '../apiConfig.js'\nimport React from \"react\";\nimport { useState, useEffect } from \"react\";\nimport { Container, Row, Col, Card, Image } from \"react-bootstrap\";\nimport axios from \"axios\";\nimport { Link } from 'react-router-dom';\nimport LinesEllipsis from 'react-lines-ellipsis'\n\nexport default function Home(props) {\n  const [articles, setArticles] = useState([])\n\n  useEffect